<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff2c2934" android:endColor="#ff2c2934" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff35a1e9" android:endColor="#ff35a1e9" android:angle="270.0" android:centerColor="#ff35a1e9" />
            </shape>
        </clip>
    </item>
</layer-list>